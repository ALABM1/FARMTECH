<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="tn.edu.esprit.gui.DisplayQRController">
   <children>
      <ImageView fitHeight="550.0" fitWidth="963.0" layoutY="-7.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../../../images/background_image.png" />
         </image>
      </ImageView>
      <ImageView fx:id="qrImageView" fitHeight="248.0" fitWidth="426.0" layoutX="280.0" layoutY="40.0" pickOnBounds="true" preserveRatio="true" />
      <Button layoutX="355.0" layoutY="315.0" mnemonicParsing="false" onAction="#telechargerAction" prefHeight="44.0" prefWidth="118.0" style="-fx-background-color: #000000;" styleClass="B" text="Telecharger" textFill="WHITE" />
      <Button fx:id="loginButton" layoutX="500.0" layoutY="315.0" mnemonicParsing="false" onAction="#LoginAction" prefHeight="44.0" prefWidth="118.0" style="-fx-background-color: #000000;" text="Login" textFill="WHITE" />
   </children>
</AnchorPane>
